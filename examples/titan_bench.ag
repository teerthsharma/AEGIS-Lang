// Titan Benchmark: Fibonacci Loop
// Stress tests variable access, arithmetic, and while loops.

let a = 0.0
let b = 1.0
let temp = 0.0
let i = 0.0
let n = 1000000.0 // 1 Million iterations

// The Loop
while i < n {
    // Simple Fib-like calc, but modulo or reset to prevent overflow for benchmark
    temp = a + b
    a = b
    b = temp
    
    // Reset if too big to keep it running hot but valid floats
    if a > 10000.0 {
        a = 0.0
        b = 1.0
    }
    
    i = i + 1.0
    
    // Minimal print to avoid I/O bottleneck
    // print a
}

render i { label: "Done" }~
